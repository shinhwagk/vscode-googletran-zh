{
    "name": "vscode-translate",
    "displayName": "vscode-translate",
    "description": "simaple translate, en -> zh",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.31.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "yarn run compile "
    },
    "devDependencies": {
        "@types/node": "^11.9.3",
        "@types/request": "^2.48.1",
        "vscode": "^1.1.29"
    },
    "dependencies": {
        "request": "^2.88.0"
    }
}
