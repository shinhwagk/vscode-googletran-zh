{
    "name": "vscode-googletran-zh",
    "displayName": "vscode-googletran-zh",
    "description": "simaple translate, en -> zh-cn",
    "version": "0.0.2",
    "publisher": "shinhwagk",
    "preview": true,
    "engines": {
        "vscode": "^1.31.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/shinhwagk/vscode-googletran-zh"
    },
    "categories": [
        "Other"
    ],
    "license": "MIT",
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.translate.document",
                "title": "vscode-transalte: transalte document"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn compile",
        "compile": "webpack --mode production --config webpack.config.ts",
        "watch": "webpack --mode development --config webpack.config.ts --watch",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "jest"
    },
    "devDependencies": {
        "@types/jest": "^24.0.5",
        "@types/node": "^11.9.4",
        "@types/request": "^2.48.1",
        "@types/webpack": "^4.4.24",
        "jest": "^24.1.0",
        "ts-jest": "^23.10.5",
        "ts-loader": "^5.3.3",
        "ts-node": "^8.0.2",
        "typescript": "^3.3.3",
        "vscode": "^1.1.29",
        "webpack": "^4.29.4",
        "webpack-cli": "^3.2.3"
    },
    "dependencies": {
        "request": "^2.88.0"
    }
}
