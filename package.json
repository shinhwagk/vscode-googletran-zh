{
    "name": "vscode-googletran-zh",
    "displayName": "vscode-googletran-zh",
    "description": "simaple translate, en -> zh-cn",
    "version": "0.0.1",
    "preview": true,
    "engines": {
        "vscode": "^1.31.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.translate",
                "title": "vscode-transalte: transalte"
            },
            {
                "command": "exttension.translate.document",
                "title": "vscode-transalte: transalte document"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "webpack --mode production --config webpack.config.ts",
        "watch": "webpack --mode development --config webpack.config.ts --watch",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/got": "^9.4.0",
        "@types/node": "^11.9.3",
        "@types/webpack": "^4.4.24",
        "ts-loader": "^5.3.3",
        "ts-node": "^8.0.2",
        "typescript": "^3.3.3",
        "vscode": "^1.1.29",
        "webpack": "^4.29.3",
        "webpack-cli": "^3.2.3"
    },
    "dependencies": {
        "got": "^9.6.0"
    }
}
